{
  "name": "lpbayliss-nextjs-starter",
  "version": "1.0.0",
  "description": "A React starter kit using next.js, typescript, emotion, tailwindcss, jest + enzyme, storybook and prettier.",
  "repository": "https://github.com/lpbayliss/nextjs-starter/blob/master/package.json",
  "main": "index.js",
  "scripts": {
    "develop": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook:start": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml}\""
  },
  "husky": {
    "pre-commit": "lint-staged",
    "pre-push": "npm run test"
  },
  "lint-staged": {
    "*": [
      "npm run prettier:fix",
      "git add"
    ]
  },
  "keywords": [
    "react",
    "javascript",
    "typescript",
    "emotion",
    "nextjs",
    "taiwlind",
    "jest"
  ],
  "author": "Luke Bayliss",
  "license": "ISC",
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@storybook/addon-actions": "^5.3.10",
    "@storybook/addon-links": "^5.3.10",
    "@storybook/addons": "^5.3.10",
    "@storybook/react": "^5.3.10",
    "@types/enzyme": "^3.10.4",
    "@types/qrcode.react": "^1.0.0",
    "next": "^11.1.3",
    "qrcode.react": "^1.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "tailwindcss": "^1.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@storybook/addon-knobs": "^5.3.10",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/storybook__react": "^5.2.1",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^2.8.0",
    "babel-preset-react-app": "^9.1.1",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.12.0",
    "tailwind.macro": "^1.0.0-alpha.10",
    "typescript": "^3.7.5"
  }
}
